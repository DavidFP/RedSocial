        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/notifications.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>wall</from-outcome>
            <to-view-id>/wall.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>notifications</from-outcome>
            <to-view-id>/notifications.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
